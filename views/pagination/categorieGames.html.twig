<div style="margin: 1% 1%;padding-left: 35%;">
	<p class="wp-link-pages">
		Pages: 
		<a href="/flashgames/categorie/{{categorie}}/1" style="width: 45px;">first</a>
		{% if not (barepagination.actuel == 1) %}
			<a href="/flashgames/categorie/{{categorie}}/{{barepagination.actuel-1}}">previous</a>
		{% endif %}
		{% for page in barepagination.pages %}
			{% if barepagination.actuel == page %}
				
				{{page}}
			
			{% else %}

				<a href="/flashgames/categorie/{{categorie}}/{{page}}">{{page}}</a> 

			{% endif %}
		{% endfor %}
		
		{% if not (barepagination.actuel == nbrPage) %}
			<a href="/flashgames/categorie/{{categorie}}/{{barepagination.actuel+1}}" style="width: 45px;">next</a>
		{% endif %}
		<a href="/flashgames/categorie/{{categorie}}/{{nbrPage}}" style="width: 45px;">last</a>

	</p>
</div>